<div class="container_new_patient w-100 px-3">
  <% @memberships.each do |membership| %>
    <%= simple_form_for [@team, membership] do |f| %>
      <%= f.input :email,
                  placeholder: 'Email du participant',
                  :label => false %>
      <h2><%= f.button :submit, "Ajouter un participant", class: "btn-add-treatment" %></h2>
     <% end %>
  <% end %>
